{
  "name": "@tilt.dev/tilt-inspector",
  "version": "0.1.4",
  "description": "A small debugging server to help inspect Tilt state",
  "repository": "https://github.com/tilt-dev/tilt-inspector",
  "author": "Nick Santos <nick@tilt.dev>",
  "license": "Apache-2.0",
  "private": false,
  "files": [
    ".next",
    "public",
    "index.js",
    "start.js"
  ],
  "dependencies": {
    "@kubernetes/client-node": "^0.14.3",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "next": "^10.0.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "universal-cookie": "^4.0.4"
  },
  "devDependencies": {
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "prettier": "^2.2.1",
    "typedoc": "^0.20.35",
    "typescript": "^4.2.4"
  },
  "bin": {
    "tilt-inspector": "./start.js"
  },
  "scripts": {
    "prepublish": "next build",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier --loglevel error --write \"./src/**/*.ts\" \"./src/**/*.js\"",
    "lint": "tslint --project \".\" && prettier --check \"./src/**/*.ts\" && tslint --project \"./examples/typescript\"",
    "docs": "typedoc src/gen/api",
    "genapi": "./genapi.sh"
  }
